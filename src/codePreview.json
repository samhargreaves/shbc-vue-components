{
    "Spinner": [
        "import { Spinner } from \"@shbc-vue-components\";",
        "",
        "<Spinner />"
    ],
    "Modal": [
        "import { Modal } from \"@shbc-vue-components\";",
        "",
        "<SecondaryButton @click=\"modal = true\">Open Modal</SecondaryButton>",
        "<Modal :show=\"modal\" @close=\"modal = false\">",
        "   <div class=\"p-4\">My modal content</div>",
        "</Modal>"
    ],
    "Logs": [
        "import { Logs } from \"@shbc-vue-components\";",
        "",
        "<Logs header=\"myLogs\" :logs=\"{}\" />"
    ],
    "Dropdown": [
        "import { Dropdown, DropdownLink } from \"@shbc-vue-components\";",
        "",
        "<Dropdown align=\"left\" content-classes=\"p-2 bg-gray-200\">",
        "    <template #trigger>Click me</template>",
        "    <template #content>",
        "        <ul>",
        "            <li><DropdownLink href=\"#url1\">item 1</DropdownLink></li>",
        "            <li><DropdownLink href=\"#url2\">item 2</DropdownLink></li>",
        "            <li><DropdownLink href=\"#url3\">item 3</DropdownLink></li>",
        "        </ul>",
        "    </template>",
        "</Dropdown>"
    ],
    "DescriptionList": [
        "import { DescriptionList, DescriptionListItem } from \"@shbc-vue-components\";",
        "",
        "<DescriptionList>",
        "   <DescriptionListItem editable>my description list</DescriptionListItem>",
        "   <DescriptionListItem editable label=\"my label\" value=\"random\" />",
        "</DescriptionList>"
    ],
    "Alert":[
        "import { Alert } from \"@shbc-vue-components\";",
        "",
        "<Alert>My alert message</Alert>"
    ],
    "TableItemCard": [
        "import { TableItemCard } from \"@shbc-vue-components\";",
        "",
        "<TableItemCard",
        "  :itemId=\"{ id: 2, prefix: false }\"",
        "  :pills=\"[{ text: 'myStatus' }]\"",
        "  :options=\"[",
        "    {",
        "      text: 'No. Items: 342',",
        "    },",
        "    {",
        "      text: 'Sold: 23',",
        "    },",
        "  ]\"",
        "  timeStamp=\"12.10.2024\"",
        "  :title=\"{ text: 'Some Product Name' }\"",
        "  extraText=\"my note\"",
        "  :amount=\"{ text: 'Amount: ', amount: 23.45 }\"",
        "/>"
    ],
    "Table": [
        "import { Table } from \"@shbc-vue-components\";",
        "",
        "<Table",
        "  class=\"w-full\"",
        "  :total=\"30\"",
        "  :links=\"[",
        "    { url: '#table', label: 'label - 1' },",
        "    { url: '#table', label: 'label - 2' },",
        "    { url: '#table', label: 'label - 3' },",
        "    { url: '#table', label: 'label - 4' },",
        "  ]\"",
        "  :sticky=\"true\"",
        ">",
        "  <Thead>",
        "    <Tr>",
        "      <Th>Header 1</Th>",
        "      <Th>Header 2</Th>",
        "      <Th>Header 3</Th>",
        "    </Tr>",
        "  </Thead>",
        "  <tbody>",
        "    <Tr>",
        "      <Td>Row 1 - Col 1</Td>",
        "      <Td>Row 1 - Col 2</Td>",
        "      <Td>Row 1 - Col 3</Td>",
        "    </Tr>",
        "    <Tr>",
        "      <Td>Row 2 - Col 1</Td>",
        "      <Td>Row 2 - Col 2</Td>",
        "      <Td>Row 2 - Col 3</Td>",
        "    </Tr>",
        "    <Tr>",
        "      <Td>Row 3 - Col 1</Td>",
        "      <Td>Row 3 - Col 2</Td>",
        "      <Td>Row 3 - Col 3</Td>",
        "    </Tr>",
        "    <TrCollapse>",
        "      <Td>Row 4 - Col 1</Td>",
        "      <Td>Row 4 - Col 2</Td>",
        "      <Td>Row 4 - Col 3</Td>",
        "    </TrCollapse>",
        "    <TrCollapseHandler>",
        "      <Td>Row 4 - Col 1</Td>",
        "      <Td>Row 4 - Col 2</Td>",
        "      <Td>Row 4 - Col 3</Td>",
        "    </TrCollapseHandler>",
        "  </tbody>",
        "</Table>"
    ],
    "TextInput": [
        "import { TextInput } from \"@shbc-vue-components\";",
        "",
        "<TextInput :form=\"form\" field=\"name\" label=\"Name\" modelValue=\"myValue\" />"
    ],
    "Textarea": [
        "import { Textarea } from \"@shbc-vue-components\";",
        "",
        "<Textarea :form=\"form\" field=\"note\" label=\"Note\" rows=\"4\" />"
    ],
    "SelectMultiple": [
        "import { SelectMultiple } from \"@shbc-vue-components\";",
        "",
        "<SelectMultiple",
        "   id=\"ba_country\"",
        "   label=\"Country\"",
        "   :list=\"[",
        "     { id: 1, name: 'India' },",
        "     { id: 2, name: 'USA' },",
        "     { id: 3, name: 'UK' },",
        "   ]\"",
        "   optionValue=\"id\"",
        "   optionText=\"name\"",
        "   placeholder=\"Select Country\"",
        "   field=\"ba_country\"",
        "   :form=\"form\"",
        "/>"
    ],
    "SearchSelect": ["---"],
    "RadioButton": [
        "import { RadioButton } from \"@shbc-vue-components\";",
        "",
        "<div class=\"flex gap-4\">",
        "   <RadioButton :value=\"null\" v-model=\"form.option\" id=\"radioDefault01\" :isChecked=\"true\" name=\"option\" label=\"All\"/>",
        "   <RadioButton value=\"In Stock\" v-model=\"form.option\" id=\"radioDefault02\" name=\"option\" label=\"In Stock\"/>",
        "   <RadioButton value=\"eng_note\" v-model=\"form.option\" id=\"radioDefault03\" name=\"option\" label=\"With Engineer Notes\"/>",
        "</div>"
    ],
    "InputLabel": [
        "import { InputLabel } from \"@shbc-vue-components\";",
        "",
        "<div class=\"flex gap-4 flex-col\">",
        "   <InputLabel value=\"My Label - 1\" />",
        "   <InputLabel value=\"My Label - 2\" :required=\"true\" />",
        "   <InputLabel value=\"My Label - 2\" sublabel=\"subLabel\" />",
        "</div>"
    ],
    "InputError": [
        "import { InputError } from \"@shbc-vue-components\";",
        "",
        "<div class=\"flex gap-4 flex-col\">",
        "   <textarea class=\"-mb-3\">My input</textarea>",
        "   <InputError message=\"Something is wrong\" />",
        "</div>"
    ],
    "DropdownSearchBar": [
        "import { DropdownSearchbar } from \"@shbc-vue-components\";",
        "",
        "<div class=\"flex gap-4 items-center\">",
        "   <DropdownSearchbar",
        "       :options=\"[",
        "           'my first option',",
        "           'some random option',",
        "       ]\"",
        "   />",
        "   <DropdownSearchbar placeholder=\"find me\" />",
        "</div>"
    ],
    "Checkbox": [
        "import { Checkbox } from \"@shbc-vue-components\";",
        "",
        "const form = useForm({",
        "  checkbox: false,",
        "});",
        "",
        "<Checkbox value=\"value\" v-model:checked=\"form.checkbox\" />",
        "<div class=\"flex gap-4 items-center\">",
        " <Checkbox value=\"value\" @update:checked=\"(value) => (form.checkbox = value)\"/>",
        " {{ form.checkbox ? \"checked\" : \"notChecked\" }}",
        " <Checkbox checked />",
        "</div>"
    ],
    "FileInput": [
        "import { Input } from \"@shbc-vue-components\";",
        "",
        "<div class=\"flex gap-4 items-center\">",
        "<FileDropZoneInput :form=\"form\" field=\"img\" />",
        "<FileDropZoneInput :form=\"form\" field=\"img\" label=\"MyLabel\" />",
        "</div>"
    ],
    "Switch": [
        "import { Input } from \"@shbc-vue-components\";",
        "",
        "<div class=\"flex gap-4 items-center\">",
        "   <Input :form=\"form\" field=\"subscribe\" type=\"checkbox\" />",
        "   <Input :form=\"form\" field=\"subscribe\" type=\"checkbox\" noLabel />",
        "   <Input :form=\"form\" field=\"subscribe\" type=\"checkbox\" switchDescription=\"Description\" label=\"MyLabel\"/>",
        "</div>"
    ],
    "Input": [
        "import { Input } from \"@shbc-vue-components\";",
        "",
        "<div class=\"grid w-full gap-5 py-2 grid-cols-3\">",
        "   <Input :form=\"form\" field=\"name\" />",
        "   <Input :form=\"form\" field=\"name\" disabled />",
        "   <Input :form=\"form\" field=\"name2\" noLabel />",
        "   <Input :form=\"form\" field=\"name3\" addon=\"Name\" />",
        "   <Input :form=\"form\" field=\"name4\" submitBtn=\"Action\" />",
        "   <Input :form=\"form\" field=\"name5\" whatsApp=\"#myUrl\" />",
        "</div>"
    ],
    "Buttons": [
        "import { SubmitButton, PrimaryButton, SecondaryButton, DangerButton, LinkDropdownButton, LinkDropdownButtonItem } from \"@shbc-vue-components\";",
        "",
        "<div class=\"flex w-full gap-5 py-2\">",
        "   <SubmitButton :form=\"form\">Submit</SubmitButton>",
        "   <PrimaryButton>Primary</PrimaryButton>",
        "   <SecondaryButton>Secondary</SecondaryButton>",
        "   <DangerButton>Danger</DangerButton>",
        "   <LinkButton href=\"#mylink1\">myLink button</LinkButton>",
        "   <LinkDropdownButton title=\"myLink Dropdown button\">",
        "       <LinkDropdownButtonItem title=\"MyTitle\" href=\"#myRoute\" />",
        "   </LinkDropdownButton>",
        "</div>"
    ],
    "Section": [
        "import Section from \"./@shbc-vue-components/Section.vue\";",
        "",
        "<Section header=\"MySection\" :open=\"true\">",
        "        My Section Data",
        "</Section>"
    ],
    "CollapsibleSection": [
        "import CollapsibleSection from \"./@shbc-vue-components/CollapsibleSection.vue\";",
        "",
        "<CollapsibleSection header=\"MyCollapsibleSection\">",
        "   My Section Data",
        "</CollapsibleSection>"
    ]
}
